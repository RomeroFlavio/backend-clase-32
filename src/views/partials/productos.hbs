<div>
    <table>
        <tr>
            <th>ID</th>
            <th>Title</th>
            <th>Price</th>
            <th>Thumbnail</th>
        </tr>
            {{#each element}}
            <tr>
                <td>{{id}}</td>
                <td>{{title}}</td>
                <td>{{price}}</td>
                <td><img src="{{thumbnail}}" alt="{{title}}" width="50"></td>
            </tr>
            {{/each}}
            <h2>{{message}}</h2>
    </table>
    <a href="/">Volver al formulario</a>
</div>